index=syf_apps 
source="/appbin/atlassian/application-data/bitbucket/log/*.log" 
log_level=ERROR 
| rex field=_raw "(?i)Caused by:\s+(?<exception_class>[^\s:]+)(?::\s*(?<exception_message>.*))?"
| stats count AS occurrences BY exception_class exception_message
| sort - occurrences
| table occurrences exception_class exception_message