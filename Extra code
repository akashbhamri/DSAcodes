while IFS=$'\n' read -r dest; do
  echo "Processing destination: $dest"

  if [ ! -f "$dest" ]; then
    echo "Dest file $dest not available. So certs are not imported in $dest."
  else
    echo "Checking if alias $alias_name exists in $dest."

    # Check if the alias already exists in the keystore
    if keytool -list -keystore "$dest" -storepass changeit -alias "$alias_name" >/dev/null 2>&1; then
      echo "Alias $alias_name already exists in $dest. Certificate not imported."
      import_status="Alias already exists"
    else
      echo "Importing $source_file into $dest with alias $alias_name."

      # Attempt to import the certificate
      keytool -import -trustcacerts -alias "$alias_name" -file "$source_file" -keystore "$dest" -storepass changeit -noprompt

      # Check if the import was successful
      if [ $? -eq 0 ]; then
        echo "Import successful in $dest."
        import_status="Success"
      else
        echo "Import not successful in $dest."
        import_status="Failed"
      fi
    fi
  fi
  # (Optional) Log the status to a CSV or another log file
done < "$destination_file"
